{
  "manifest_version": 2,

  "name": "QuickMyAnimeList - By FoxInFlame",
  "description": "Quick edit/add/delete animes on your MyAnimeList account!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "http://myanimelist.net/api",
    "storage"
  ],
  
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  
  "icons": {
		"128": "icon.png"
	}
}