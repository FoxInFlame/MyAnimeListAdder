{
  "manifest_version": 2,

  "name": "MyAnimeListAdder - By FoxInFlame",
  "description": "Add an Anime to your MyAnimeList",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "http://myanimelist.net/api"
  ],
  
  "icons": {
		"128": "icon.png"
	}
}